{
  "name": "first-vue",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/guillaumebouhier/first-vue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "node ./node_modules/vue-gh-pages/index.js"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuetify": "^2.5.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@vue/babel-preset-app": "^5.0.6",
    "@vue/cli-plugin-babel": "^5.0.0",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "sass": "^1.17.4",
    "sass-loader": "^13.0.0",
    "vue-cli-plugin-vuetify": "^2.5.0",
    "vue-gh-pages": "^1.5.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
